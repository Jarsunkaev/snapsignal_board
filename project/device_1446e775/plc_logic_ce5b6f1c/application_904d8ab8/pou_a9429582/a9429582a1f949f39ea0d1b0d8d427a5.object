{"payload":{"meta":{"Graph":{"@Type":"81297157","@Value":{"Guid":"(Guid)a9429582-a1f9-49f3-9ea0-d1b0d8d427a5","ParentGuid":"(Guid)904d8ab8-e1a4-4558-a196-64a2bc79aa77","Name":"(string)POU","Properties":{"@Type":"2c41fa04:IDictionary","@Value":{}},"TypeGuid":"(Guid)6f9dac99-8de1-4efc-8465-68ac443b7d08","EmbeddedTypeGuids":{"@Type":"[Guid]","@Value":["(Guid)a9ed5b7e-75c5-4651-af16-d2c27e98cb94","(Guid)3b83b776-fb25-43b8-99f2-3c507c9143fc"]}}},"TypeInfos":{"2c41fa04":"{2c41fa04-1834-41c1-816e-303c7aa2c05b}","81297157":"{81297157-7ec9-45ce-845e-84cab2b88ade}","Guid":"System.Guid","string":"System.String"}},"object":{"Graph":{"@Type":"6f9dac99","@Value":{"SpecialFunc":{"@Type":"0db3d7bb:Enum","@Value":"None"},"Implementation":{"@Type":"3b83b776","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)\n// Timer set to prevent PageIndex incrementing/decrementing too fast when buttons are pressed\nSwitchTimer(IN := (display_41_button = 1 OR display_41_button = 2), PT := SwitchDelay);\n\n// Making sure POUs don't run simultaneously\nCASE g_ActiveDevice OF\n    0: CURRENT();\n    1: VIBRATION();\n    2: LPOS();\n    3: ; // No POU runs when on home screen\nEND_CASE\n\nIF CurrentVisu = 'Current_screen' THEN\n    g_ActiveDevice := 0;\nELSIF CurrentVisu = 'Vibration_temp_screen' THEN\n    g_ActiveDevice := 1;\nELSIF CurrentVisu = 'Linear_pos_screen' THEN\n    g_ActiveDevice := 2;\nELSIF CurrentVisu = 'Main_screen' THEN\n    g_ActiveDevice := 3;\n    display_41_colour := 1;\n    display_40_colour := 1;\n    display_40 := 0;\n    display_41 := 0;\nEND_IF\n\n// Increment/Decrement PageIndex when display_41_button is pressed logic\nIF SwitchTimer.Q THEN\n    IF display_41_button = 2 THEN // Up button pressed\n        PageIndex := PageIndex + 1;\n\t\tactuated_41_color := 5;\n        IF PageIndex > 4 THEN\n            PageIndex := 1; // Wrap around to the first page\n        END_IF\n    ELSIF display_41_button = 1 THEN // Down button pressed\n        PageIndex := PageIndex - 1;\n\t\tactuated_41_color := 12;\n        IF PageIndex < 1 THEN\n            PageIndex := 4; // Wrap around to the last page\n        END_IF\n    END_IF\n    // Reset the timer\n    SwitchTimer(IN := FALSE);\nEND_IF\n\n\n// Logic to switch the frame content based on PageIndex\nCASE PageIndex OF\n    1: CurrentVisu := 'Main_screen';\n    2: CurrentVisu := 'Current_screen';\n    3: CurrentVisu := 'Linear_pos_screen';\n    4: CurrentVisu := 'Vibration_temp_screen';\nEND_CASE\n","LineInfoPersistence":"(string)a9429582-a1f9-49f3-9ea0-d1b0d8d427a5_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)PROGRAM POU\nVAR\n\t g_ActiveDevice : INT := 3;\n\t PageIndex :INT;\n\t SwitchTimer: TON;\n\t SwitchDelay : TIME := T#1000MS;\nEND_VAR\n","LineInfoPersistence":"(string)a9429582-a1f9-49f3-9ea0-d1b0d8d427a5_Decl_LineIds"}}}},"UniqueIdGenerator":"(string)257","POULevel":{"@Type":"8e575c5b:Enum","@Value":"Standard"},"ChildObjectGuids":{"@Type":"ArrayList:IList","@Value":[]},"AddAttributeSubsequent":"(bool)False"}},"TypeInfos":{"0db3d7bb":"{0db3d7bb-cde0-4416-9a7b-ce49a0124323}","3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","6f9dac99":"{6f9dac99-8de1-4efc-8465-68ac443b7d08}","8e575c5b":"{8e575c5b-1d37-49c6-941b-5c0ec7874787}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","ArrayList":"System.Collections.ArrayList","bool":"System.Boolean","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}